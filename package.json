    "cross-env": "^7.0.3",
    "element-ui": "^2.15.6",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.2.0",
    "graphql-tools": "^8.2.0",
    "nuxt": "^2.15.8",
    "nuxt-property-decorator": "^2.9.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "vue-class-component": "^7.2.6",
    "vuetify": "^2.6.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^2.1.0",
    "@types/express": "^4.17.13",
